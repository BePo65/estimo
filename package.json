{
  "name": "estimo",
  "version": "2.0.1",
  "description": "Evaluates how long the browser will execute your javascript code.",
  "main": "index.js",
  "scripts": {
    "install": "node ./scripts/findChrome.js",
    "prettier": "prettier --write -l 'src/**/*.{js,jsx}'",
    "eslint": "eslint --cache src",
    "test": "ava --exit",
    "test:launch": "ESTIMO_DEBUG=true node ./test/__mock__/test-launch.js",
    "test:rti-origin": "node ./test/__mock__/origin-rti-test.js",
    "docker:b": "docker build -t estimo .",
    "docker:t": "docker run --rm --init -v $(pwd)/temp:/app/temp estimo"
  },
  "dependencies": {
    "nanoid": "^2.0.4",
    "puppeteer-core": "^1.17.0",
    "tracium": "^0.2.1",
    "yargs": "^14.0.0"
  },
  "devDependencies": {
    "ava": "^2.3.0",
    "eslint": "^6.2.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-import": "^2.17.3",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2"
  },
  "engines": {
    "node": ">=8"
  },
  "preferGlobal": true,
  "bin": {
    "estimo": "./scripts/cli.js"
  },
  "ava": {
    "files": [
      "test/**/*.js",
      "!test/__mock__/**"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(js)": [
      "yarn eslint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mbalabash/estimo.git"
  },
  "homepage": "https://github.com/mbalabash/estimo#readme",
  "bugs": {
    "url": "https://github.com/mbalabash/estimo/issues"
  },
  "keywords": [
    "chrome",
    "tracium",
    "puppeteer",
    "devtools",
    "size-limit",
    "performance",
    "js"
  ],
  "author": "mbalabash <maksim.balabash@gmail.com>",
  "license": "MIT"
}
